<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC
 "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
 "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration> 
	<context id="jabiStudioFontanelli" targetRuntime="MyBatis3">
		
		<commentGenerator>
			<!-- whether the removal of the automatically generated annotation true： 
				is ： false: No information -->
			<property name="suppressAllComments" value="false" />
			
		</commentGenerator>
		<!-- database connection ： drive 、 connection address 、 username 、 password 
			, here configuration is mysql ， can of course configuration oracle databases -->
		<jdbcConnection driverClass="org.postgresql.Driver"
			connectionURL="jdbc:postgresql://127.0.0.1:5432/jabi-studiofontanelli"
			userId="jabi" password="jabiuser">
		</jdbcConnection>

		<!-- default false， JDBC DECIMAL and NUMERIC type resolution for Integer， 
			true to JDBC DECIMAL and NUMERIC type resolution for java.math.BigDecimal -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- targetProject: generation position PO -->
		<javaModelGenerator 
					targetPackage="it.studiofontanelli.jabistudio.entity" 
					targetProject="src">
			<!-- enableSubPackages: class of the whether let schema as package suffix -->
			<property name="enableSubPackages" value="false" />
			<!-- from a database of return values be cleared before and after spaces -->
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		
		<!-- targetProject:mapper Position -->
		<sqlMapGenerator targetPackage="it.studiofontanelli.jabistudio.mapper"
			targetProject="src">
			<!-- enableSubPackages: mapping file generated by whether let schema as 
				position -->
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		<!-- targetPackage：mapper package suffix -->
		
		
		<javaClientGenerator type="XMLMAPPER" 
			targetPackage="it.studiofontanelli.jabistudio.mapper" targetProject="src">
			<!-- enableSubPackages: interface to generate the make schema specify 
				the database table -->
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		<!-- generate code -->

		<table tableName="_abi_docks_dock"></table>
		<table tableName="_abi_forms_descr"></table>
		<table tableName="_abi_forms_form"></table>

		<table  tableName="_abi_main_pof_t"></table>
		<table tableName="_abi_main_pof_r"></table>


	</context>
</generatorConfiguration>